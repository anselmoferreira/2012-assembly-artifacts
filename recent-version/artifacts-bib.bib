	Doi = {10.1038/nature02340},
	Journal = {Nature},
	Keywords = {Phylogeny, Ecosystem, Bacteria, Biofilms, Molecular Sequence Data, Species Specificity, Open Reading Frames, Sequence Analysis: DNA, Genomics, Genetic Complementation Test, Polymorphism: Single Nucleotide, Genome: Archaeal, Base Composition, Recombination: Genetic, Genome: Bacterial, Nitrogen Fixation, Environmental Microbiology, Genes: Bacterial, Carbon, Base Sequence, Archaea, RNA: Ribosomal: 16S, Genes: Archaeal},
	Language = {eng},
